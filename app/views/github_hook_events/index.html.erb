<p id="notice"><%= notice %></p>

<h1>Your Githuh Hook Events</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Type</th>
      <th scope="col">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @github_hook_events.each do |event_hooks| %>
      <tr>
        <td> <%= event_hooks.type %> </td>
        <td> <%= event_hooks.action %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
